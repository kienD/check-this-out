  "dependencies": {
    "autobind-decorator": "^2.1.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "blessed": "^0.1.81",
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "prettier": "1.10.2"
