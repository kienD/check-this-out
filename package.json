{
  "name": "check-this-out",
  "version": "1.0.0",
  "description": "command line interface to switch between recently checked out branches",
	"files": [
		"lib",
		"src"
	],
  "main": "index.js",
  "scripts": {
    "build": "babel src -d lib",
    "format": "prettier --write 'src/**/*.js'",
